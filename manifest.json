{
  "name": "Prevent reload",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension prevents page reload.",
  "background": {
        "scripts": [
            "src/background.js"
        ]
    },
  "browser_action": {
    "default_icon": "icons/reload-off.png",
    "default_title": "Prevent reload"
  },
  "permissions": [
    "*://*/"
  ]
}
